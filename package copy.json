    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "browserslist-useragent-regexp": "^3.0.0",
    "compression-webpack-plugin": "^7.1.2",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-compat": "^3.9.0",
